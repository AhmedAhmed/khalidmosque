{
  "name": "khalidmosque",
  "version": "0.1.0",
  "description": "Khalid Masjid administration dashboard. ",
  "main": "index.js",
  "scripts": {
    "start": "nodemon server/index.js",
    "build": "webpack --colors --progress --watch",
    "test": "jest"
  },
  "author": "Ahmed Ahmed",
  "license": "ISC",
  "jest": {
    "transform": {
      ".(ts|tsx)": "./node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/client/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "babel": {
    "presets": [
      [
        "latest",
        {
          "es2015": {
            "modules": false
          }
        }
      ],
      "react",
      "stage-0"
    ],
    "plugins": [
      "react-hot-loader/babel",
      "syntax-dynamic-import",
      [
        "transform-runtime",
        {
          "helpers": false,
          "polyfill": false
        }
      ],
      "lodash"
    ],
    "env": {
      "test": {
        "plugins": [
          "dynamic-import-node"
        ]
      }
    }
  },
  "dependencies": {
    "@types/history": "^4.6.2",
    "@types/node-fetch": "^1.6.7",
    "@types/react": "^16.0.30",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.14",
    "@types/react-router-dom": "^4.2.3",
    "@types/react-router-redux": "^5.0.10",
    "@types/react-test-renderer": "^16.0.1",
    "@types/redux": "^3.6.0",
    "awesome-typescript-loader": "^3.4.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.17.2",
    "bootstrap": "^3.3.7",
    "bunyan": "^1.8.10",
    "dtrace-provider": "^0.8.3",
    "express": "^4.15.3",
    "jsonwebtoken": "^8.1.1",
    "mongoose": "^4.13.11",
    "mongoose-string-query": "^0.2.5",
    "mongoose-timestamp": "^0.6.0",
    "morgan": "^1.8.2",
    "node-restful": "^0.2.6",
    "normalize.css": "^7.0.0",
    "react": "^16.2.0",
    "react-google-login": "^3.0.4",
    "react-modal": "^3.3.1",
    "react-redux": "^4.4.8",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.0.0",
    "react-router-redux": "^4.0.8",
    "react-test-renderer": "^16.2.0",
    "redux": "^3.5.2",
    "redux-thunk": "^2.2.0",
    "restify": "^4.3.0",
    "restify-errors": "^5.0.0",
    "restify-jwt": "^0.4.0",
    "restify-plugins": "^1.6.0",
    "source-map-loader": "^0.2.3",
    "webpack": "^2.6.1"
  },
  "devDependencies": {
    "@types/jest": "^22.2.0",
    "amd-define-factory-patcher-loader": "^1.0.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-jest": "^22.4.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.26.4",
    "enzyme": "^2.8.2",
    "file-loader": "^0.8.4",
    "flow-bin": "^0.37.4",
    "history": "^1.13.1",
    "image-webpack-loader": "^3.4.2",
    "jest": "^22.4.2",
    "jest-webpack": "^0.4.0",
    "jquery": "^2.1.4",
    "jsx-loader": "^0.13.2",
    "node-sass": "^3.13.1",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^16.2.0",
    "react-scripts": "^0.6.0",
    "sass-loader": "^3.1.1",
    "sizzle": "^2.2.1",
    "style-loader": "^0.13.2",
    "ts-jest": "^22.4.1",
    "typescript": "^2.6.2",
    "uglify-loader": "^1.3.0"
  }
